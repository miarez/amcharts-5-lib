{
  "type": "mixed_series",
  "container": "chartdiv",
  "dataLoader": {
    "type": "csv",
    "url": "./data/mixed-series-1.csv",
    "delimiter": ","
  },
  "engine": {
    "type": "XY",
    "categoryField": "month",
    "axes": {
      "x": {
        "id": "x",
        "type": "date",
        "grid": false,
        "position": "bottom",
        "title": "Months of the Year"
      },
      "y": [
        { "id": "y", "type": "value", "grid": true, "position": "left" },
        {
          "id": "y2",
          "type": "value",
          "position": "right",
          "grid": true,
          "min": 100,
          "max": 500,
          "title": "Margin"
        }
      ]
    },
    "series": [
      {
        "field": "revenue",
        "name": "Revenue",
        "geom": "line",
        "axis": "y",
        "xAxisId": "x",
        "xField": "month"
      },
      {
        "field": "gp_l7",
        "name": "Gp L7",
        "geom": "column",
        "axis": "y2",
        "xAxisId": "x",
        "xField": "month"
      },
      {
        "field": "gp_l8_14",
        "name": "Gp L8 14",
        "geom": "column",
        "axis": "y2",
        "xAxisId": "x",
        "xField": "month"
      }
    ]
  },
  "theme": { "animated": true, "mode": "Dark", "background": "#000000" },
  "legend": { "enabled": false },
  "cursor": {
    "enabled": false,
    "behavior": "none",
    "showLineX": true,
    "showLineY": true,
    "xAxisTooltip": { "enabled": true, "categoryFormat": "{value}" }
  },
  "scrollbars": { "x": { "enabled": false }, "y": { "enabled": false } }
}
