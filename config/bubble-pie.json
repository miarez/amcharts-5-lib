{
  "id": "bubble-pie",
  "family": "bubble-pie",
  "container": "chartdiv",
  "data": {
    "type": "csv",
    "url": "./data/bubble-pie.csv"
  },
  "fields": {
    "x": "x",
    "y": "y",
    "radius": "radius",
    "pieFields": ["part_a", "part_b", "part_c"],
    "pieLabels": ["A", "B", "C"]
  },
  "axes": {
    "x": {
      "min": 0,
      "max": 100
    },
    "y": {
      "min": 0,
      "max": 100
    }
  },
  "options": {
    "title": "Bubble Chart with Pie Bullets",
    "minBulletSize": 40,
    "maxBulletSize": 100,
    "minRadiusValue": 20,
    "maxRadiusValue": 90
  },
  "decorators": {
    "cursor": {
      "enabled": true
    },
    "scrollbarX": {
      "enabled": false
    },
    "legend": {
      "enabled": false
    }
  }
}
